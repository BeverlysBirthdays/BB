<%= simple_form_for(@agency) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs row">
  	<div class='col s6'>
  		<%= f.input :name %>
  	</div>
  </div>
  <div class='form-inputs row'>
  	<div class='col s6'>
  		<%= f.input :active, as: :boolean, boolean_style: :inline%>
  	</div>
  </div>

  <div class="form-actions row">
    <%= f.button :submit, class: 'col s3 pink white-text' %>
  </div>
  
<% end %>
